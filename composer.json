{
    "name": "yoganandgopalan/laravel-swagger",
    "description": "Auto generates the swagger documentation for a laravel project",
    "license": "MIT",
    "authors": [
        {
            "name": "Yoganand G",
            "email": "yoganand_19_19@yahoo.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "yoganandgopalan\\LaravelSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "yoganandgopalan\\LaravelSwagger\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-yaml": "Required to use the YAML output option"
    },
    "extra": {
        "laravel": {
            "providers": [
                "yoganandgopalan\\LaravelSwagger\\SwaggerServiceProvider"
            ]
        }
    }
}

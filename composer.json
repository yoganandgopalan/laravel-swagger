{
    "name": "vigneshc91/laravel-swagger",
    "description": "Auto generates the swagger documentation for a laravel project",
    "license": "MIT",
    "authors": [
        {
            "name": "Vignesh C",
            "email": "vignesh.jag@gmail.com"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "6.*"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Vigneshc91\\LaravelSwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vigneshc91\\LaravelSwagger\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "ext-yaml": "Required to use the YAML output option"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vigneshc91\\LaravelSwagger\\SwaggerServiceProvider"
            ]
        }
    }
}
